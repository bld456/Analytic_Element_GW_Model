function [Omega] = Omega_total(z,w0,zl,rl,Ql,Phi,alpha,numLakes)
Omega = 0;

for i = 1:numLakes
    rsq1= rl(1) * rl(1);
    rsq2 = rl(2) * rl(2);

   
   if  (z - zl(1)) * conj(z-zl(1)) < rsq1       
     Omega = Omega + Phi(1);
   
   elseif  (z - zl(2)) * conj(z-zl(2)) < rsq2
     Omega = Omega + Phi(2);
    else 
       
       
   Omega = Omega + Omega_well(z, zl(i), rl(i),Ql(i))  +conj(w0)* rl(i)*rl(i)/(z-zl(i)) - w0* z + alpha(i)^2/(z-); ;
        
        end
    
end

end

